<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.osman.mitmproxy</string>

  <key>ProgramArguments</key>
  <array>
    <string>/opt/homebrew/bin/mitmdump</string>
    <string>-s</string>
    <string>claude_capture.py</string>
    <string>--listen-port</string>
    <string>8080</string>
    <string>--mode</string>
    <string>regular</string>
    <string>-q</string>
  </array>

  <key>WorkingDirectory</key>
  <string>/Users/osman/Developer/infrastructure/mitmproxy</string>

  <key>RunAtLoad</key>
  <true/>

  <key>KeepAlive</key>
  <true/>

  <key>StandardOutPath</key>
  <string>/Users/osman/logs/mitmproxy.log</string>

  <key>StandardErrorPath</key>
  <string>/Users/osman/logs/mitmproxy.log</string>

  <key>EnvironmentVariables</key>
  <dict>
    <key>PATH</key>
    <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
  </dict>
</dict>
</plist>
