<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.osman.transcript-ingest</string>

  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>/Users/osman/Developer/infrastructure/mitmproxy/ingest.sh</string>
  </array>

  <key>WorkingDirectory</key>
  <string>/Users/osman/Developer/infrastructure/mitmproxy</string>

  <key>StartInterval</key>
  <integer>300</integer>

  <key>StandardOutPath</key>
  <string>/Users/osman/logs/transcript-ingest.log</string>

  <key>StandardErrorPath</key>
  <string>/Users/osman/logs/transcript-ingest.log</string>

  <key>EnvironmentVariables</key>
  <dict>
    <key>PATH</key>
    <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
  </dict>
</dict>
</plist>
